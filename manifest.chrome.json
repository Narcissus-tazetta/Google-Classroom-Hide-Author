{
    "manifest_version": 3,
    "name": "Classroom Polish",
    "version": "1.0.1",
    "description": "Google ClassroomのUIの改善します",
    "permissions": [],
    "host_permissions": ["https://classroom.google.com/*"],
    "content_scripts": [
        {
            "matches": ["https://classroom.google.com/*"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}
